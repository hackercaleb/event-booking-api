{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "no-unused-vars": "off",
    "indent": "off",
    "object-curly-newline": "off",
    "guard-for-in": "off",
    "operator-linebreak": "off",
    "no-loop-func": "off",
    "no-restricted-syntax": "off",
    "arrow-parens": "off",
    "quotes": "off",
    "spaced-comment": "off"
  }
}
